{
    "plugins": ["plugins/markdown"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "recurseDepth": 10,
    "source": {
        "include": [".", "src", "src/modules", "src/utils", "src/udc", "samples"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "tutorials": "samples"
    },    
    "templates": {
        "logo": {
            "url": "http://nhnent.github.io/tui.component.tree/latest/styles/logo.png",
            "width": "150px",
            "height": "13px",
            "link": "https://github.com/nhnent/tui.jsdoc-template"
        },
        "name": "Miracom UI-Framework with eXBuilder6",
        "footerText": "MIRACOM INC",
        "useCollapsibles": true,
         "tabNames": {
            "api": "API",
            "tutorials": "Samples"
        }
    }
}